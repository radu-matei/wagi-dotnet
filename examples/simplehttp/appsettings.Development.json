{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Deislabs.WAGI": "Trace",
      "Wasi.Experimental.Http": "Trace"
    }
  },
  "WASM": {
    "ModulePath": "modules",
    "Modules": {
      "/test": {
        "FileName": "optimized.wasm",
        "Environment" :{
        },
        "AllowedHosts": [
          "https://postman-echo.com/"
        ],
        "HttpMethod": "post"
      },
      "/writeblob": {
        "FileName": "blob.wasm",
        "Entrypoint": "writeblob",
        "Environment" :{
          "STORAGE_ACCOUNT":"",
          "STORAGE_MASTER_KEY" : ""
        },
        "AllowedHosts": [
          "https://<storage_account>.blob.core.windows.net"
        ],
        "HttpMethod": "post"
      },
      "/readblob": {
        "FileName": "blob.wasm",
        "Entrypoint": "readblob",
        "Environment" :{
          "STORAGE_ACCOUNT":"",
          "STORAGE_MASTER_KEY" : ""
        },
        "AllowedHosts": [
          "https://<storage_account>.blob.core.windows.net"
        ]
      }
    }
  }
}